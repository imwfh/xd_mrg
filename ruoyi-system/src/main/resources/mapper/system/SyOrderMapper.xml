	<sql id="selectSyOrderVo">
        select id, order_sn, cart_hashcode, prepay_id, trade_no, user_code, address_id, logistic_code, order_status, postscripts, subject, goods_count, shipping_id, pay_id, goods_amount, order_amount, dius_amount, refund_status, order_time, refund_no, order_type, return_flag, order_pay_time, version from sy_order
    </sql>
	
    <select id="selectSyOrderList" parameterType="SyOrder" resultMap="SyOrderResult">
        <include refid="selectSyOrderVo"/>
        <where>  
            <if test="id != null "> and id = #{id}</if>
             <if test="orderSn != null  and orderSn != '' "> and order_sn = #{orderSn}</if>
             <if test="cartHashcode != null  and cartHashcode != '' "> and cart_hashcode = #{cartHashcode}</if>
             <if test="prepayId != null  and prepayId != '' "> and prepay_id = #{prepayId}</if>
             <if test="tradeNo != null  and tradeNo != '' "> and trade_no = #{tradeNo}</if>
             <if test="userCode != null  and userCode != '' "> and user_code = #{userCode}</if>
             <if test="addressId != null  and addressId != '' "> and address_id = #{addressId}</if>
             <if test="logisticCode != null  and logisticCode != '' "> and logistic_code = #{logisticCode}</if>
             <if test="orderStatus != null "> and order_status = #{orderStatus}</if>
             <if test="postscripts != null  and postscripts != '' "> and postscripts = #{postscripts}</if>
             <if test="subject != null  and subject != '' "> and subject = #{subject}</if>
             <if test="goodsCount != null "> and goods_count = #{goodsCount}</if>
             <if test="shippingId != null  and shippingId != '' "> and shipping_id = #{shippingId}</if>
             <if test="payId != null  and payId != '' "> and pay_id = #{payId}</if>
             <if test="goodsAmount != null "> and goods_amount = #{goodsAmount}</if>
             <if test="orderAmount != null "> and order_amount = #{orderAmount}</if>
             <if test="diusAmount != null "> and dius_amount = #{diusAmount}</if>
             <if test="refundStatus != null "> and refund_status = #{refundStatus}</if>
             <if test="orderTime != null "> and order_time = #{orderTime}</if>
             <if test="refundNo != null  and refundNo != '' "> and refund_no = #{refundNo}</if>
             <if test="orderType != null "> and order_type = #{orderType}</if>
             <if test="returnFlag != null "> and return_flag = #{returnFlag}</if>
             <if test="orderPayTime != null "> and order_pay_time = #{orderPayTime}</if>
             <if test="version != null "> and version = #{version}</if>
         </where>
    </select>
    
    <select id="selectSyOrderById" parameterType="Integer" resultMap="SyOrderResult">
        <include refid="selectSyOrderVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSyOrder" parameterType="SyOrder" useGeneratedKeys="true" keyProperty="id">
        insert into sy_order
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="orderSn != null  and orderSn != ''  ">order_sn,</if>
			<if test="cartHashcode != null  and cartHashcode != ''  ">cart_hashcode,</if>
